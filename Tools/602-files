<VirtualHost *:80>
	ServerName files.profeda.org
	ServerAlias files*
	ServerAlias fichier*
	ServerAdmin webmaster@localhost

	DocumentRoot /usr/share/dokuwiki
	<Directory /usr/share/dokuwiki/>
		Options +FollowSymLinks
		AllowOverride All
		order allow,deny
		allow from localhost 127.0.0.1 192.168.0.0/16
	</Directory>

</VirtualHost>
